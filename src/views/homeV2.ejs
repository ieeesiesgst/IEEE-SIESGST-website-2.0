				  <div class="eventv2 position-absolute d-none d-lg-block"></div>
				  <div class="container pt-2 pt-md-3">
					<div class="row text-lg-start text-center ms-lg-4">
					  <div class="col-lg-4 col-12 ms-lg-5 ms-0 py-2">
						<div class="fs-3 fw-bold">OUR EVENTS</div>
					  </div>
					  <div class="col-lg-4 col-12 py-2">
						<div class="fs-6 px-4">
						  Have a look at our Ongoing and Upcoming
						  Events!
						</div>
					  </div>
					</div>
				
					
				  </div>
				  <div
					id="eventHomeCarousel"
					class="
					  carousel
					  slide
					  homeCarousel
					  position-absolute
					  translate-middle-x
					  start-50
					"
					data-bs-ride="carousel"
				  >
						<div class="carousel-inner">
							<% eventArray.forEach((res,index)=>{ %>
								<div class="carousel-item cursor-pointer <%= index==0 && "active" %>" onclick="location.href='/events'">
									<div class="card eventCard">
									<img src="<%= res.pic %>" class="card-img-top eventImg" alt="Event" />
